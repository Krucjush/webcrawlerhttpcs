<Window x:Class="webcrawlerhttp.Progress"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:webcrawlerhttp"
        mc:Ignorable="d"
        Title="Loading" Height="100" Width="210">
    <Grid>
        <AccessText HorizontalAlignment="Center">Fetching Data</AccessText>
        <ProgressBar x:Name="progressBar" IsIndeterminate="True" Width="200" Height="20"/>
    </Grid>
</Window>
